{
  "name": "template",
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "gql-codegen": "graphql-codegen --config codegen.yml --watch",
    "prettier": "prettier -c --write \"**/*.{js,jsx,ts,tsx,md,html,css,scss,svg}\"",
    "lint": "tsc --noEmit && eslint . --fix",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "ls": "umi plugin list --key"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": "npm run lint",
    "*.{js,jsx,ts,tsx,md,html,css,scss}": "npm run prettier",
    "*": "git add "
  },
  "dependencies": {
    "@ant-design/pro-layout": "^6.4.18",
    "@apollo/client": "^3.2.0",
    "@umijs/plugin-qiankun": "^2.12.1",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.2.20",
    "eslint": "^7.9.0",
    "graphql": "^15.3.0",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "umi": "^3.2.20",
    "yorkie": "^2.0.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.17.8",
    "@graphql-codegen/introspection": "1.17.8",
    "@graphql-codegen/typescript": "1.17.8",
    "@graphql-codegen/typescript-operations": "1.17.8",
    "@graphql-codegen/typescript-react-apollo": "2.0.6",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-prettier": "^3.1.4",
    "fbi-lint": "^0.0.2",
    "typescript": "^4.0.2"
  }
}
